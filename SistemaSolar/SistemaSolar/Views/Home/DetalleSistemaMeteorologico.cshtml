@model SistemaSolar.Models.SistemaMeteorologico

@{
    ViewData["Title"] = "DetalleSistemaMeteorologico";
}

<div>
    @using (Html.BeginForm("DetalleSistemaMeteorologico", "Home", FormMethod.Get))
    {
        <div class="col-lg-3"></div>
        <div class="col-lg-6">
            <h1> Sistema Meteorologico </h1>
            <form class="login-form">
                <div class="form-group">
                    @Html.Label("PeriodoSequia", "Periodos de Sequia: ", new { @class = "control-label" })
                    <div class="col-lg-9">
                        @Html.TextBoxFor(m => m.PeriodoSequia, new { @class = "form-control" })
                    </div>
                </div>

                <div class="form-group">
                    @Html.Label("", "Periodos de Lluvia", new { @class = "control-label" })
                    <div class="col-lg-9">
                        @Html.LabelFor(m => m.PeriodoLluvia, new { @class = "form-control" })

                    </div>
                </div>

                <div class="form-group">
                    @Html.Label("PeriodoPresionTemperatura: ", "Periodo Presion Temperatura", new { @class = "control-label" })
                    <div class="col-lg-9">
                        @Html.LabelFor(m => m.PeriodoPresionTemperatura, new { @class = "form-control" })
                    </div>
                </div>
            </form>
        </div>
        <div class="col-lg-3"></div>
    }
    </div>